===========================================================================
#1번 문제
===========================================================================
#include <stdio.h>

int main(){
    printf("Hello World!");
    return 0;
}
===========================================================================
#2번 문제 
===========================================================================
#include <stdio.h>

int main()
{
    printf("강한친구 대한육군\n");
    printf("강한친구 대한육군");
    return 0;
}
===========================================================================
#3번 문제
===========================================================================
#include <stdio.h>

int main (void)
{
    printf("\\    /\\\n");
    printf(" )  ( ')\n");
    printf("(  /  )\n");
    printf(" \\(__)|\n");
    return 0;
}
===========================================================================
#4번 문제
===========================================================================
#include <stdio.h>

int main(void)
{
    printf("|\\_/|\n");
    printf("|q p|   /}\n");
    printf("( 0 )\"\"\"\\\n");
    printf("|\"^\"`    |\n");
    printf("||_/=\\\\__|\n");
    return 0;
}
===========================================================================
#5번 
===========================================================================
#include <stdio.h>

int main(void)
{
    int a;
    scanf("%d%d",&a,&b);
    printf("%d\n",a+b);
    return 0;
}
===========================================================================
#6번 문제
===========================================================================
#include <stdio.h>

int main (void)
{
    int a,b;
    scanf("%d %d",&a,&b);
    printf("%d",a-b);
    return 0;
}
===========================================================================
#7번 문제
===========================================================================
#include <stdio.h>

int main ()
{
    int a,b;
    scanf("%d %d",&a,&b);
    printf("%d",a*b);
    return 0;
}
===========================================================================
#8번 문제
===========================================================================
#include <stdio.h>

int main ()
{
    int a,b;
    scanf("%d %d",&a,&b);
    printf("%d",a*b);
}
===========================================================================
#9번 문제
===========================================================================
#include <stdio.h>

int main ()
{
    double a,b;
    scanf("%lf %lf",&a,&b);
    printf("%.9lf",a/b);
    return 0;
}
