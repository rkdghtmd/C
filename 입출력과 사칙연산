===========================================================================
#2번 문제 
===========================================================================
#include <stdio.h>

int main()
{
    printf("강한친구 대한육군\n");
    printf("강한친구 대한육군");
    return 0;
}
===========================================================================
#3번 문제
===========================================================================
#include <stdio.h>

int main(void)
{
    printf("|\\_/|\n");
    printf("|q p|   /}\n");
    printf("( 0 )\"\"\"\\\n");
    printf("|\"^\"`    |\n");
    printf("||_/=\\\\__|\n");
    return 0;
}
