===========================================================================
#9번 문제
===========================================================================
#include <stdio.h>

int main ()
{
    double a,b;
    scanf("%lf %lf",&a,&b);
    printf("%.9lf",a/b);
    return 0;
}
===========================================================================
#10번 문제
===========================================================================
#include <stdio.h>

int main ()
{
    int a,b;
    scanf("%d %d",&a,&b);
    printf("%d\n",a+b);
    printf("%d\n",a-b);
    printf("%d\n",a*b);
    printf("%d\n",a/b);
    printf("%d\n",a%b);
}
===========================================================================
#11번 문제
===========================================================================
#include <stdio.h>

int main ()
{
    int a,b,c,d,e;
    scanf("%d",&a);
    scanf("%d",&b);
    c=(b%10)*a;
    d=a*((b/10)%10);
    e=a*((b/100)%10);
    printf("%d\n",c);
    printf("%d\n",d);
    printf("%d\n",e);
    printf("%d",a*b);
}
