===========================================================================
#1번 문제
===========================================================================
#include <stdio.h>

int main(){
    printf("Hello World!");
    return 0;
}
===========================================================================
#2번 문제 
===========================================================================
#include <stdio.h>

int main()
{
    printf("강한친구 대한육군\n");
    printf("강한친구 대한육군");
    return 0;
}
===========================================================================
#3번 문제
===========================================================================
#include <stdio.h>

int main (void)
{
    printf("\\    /\\\n");
    printf(" )  ( ')\n");
    printf("(  /  )\n");
    printf(" \\(__)|\n");
    return 0;
}
===========================================================================
#4번 문제
===========================================================================
#include <stdio.h>

int main(void)
{
    printf("|\\_/|\n");
    printf("|q p|   /}\n");
    printf("( 0 )\"\"\"\\\n");
    printf("|\"^\"`    |\n");
    printf("||_/=\\\\__|\n");
    return 0;
}
===========================================================================
#5번 
===========================================================================
#include <stdio.h>

int main(void)
{
    int a;
    scanf("%d%d",&a,&b);
    printf("%d\n",a+b);
    return 0;
}
