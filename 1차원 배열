==============================================================
#1번 문제
==============================================================
#include <stdio.h>

int main()
{
    int number,Case,min,max;
    min=1000000;
    max=-1000000;
    scanf("%d",&number);
    for (;number>0;number--)
    {
        scanf("%d",&Case);
        if (min>Case)
        {
            min=Case;
        }
        if (max<Case)
        {
            max=Case;
        }
    }
    printf("%d %d",min,max);
    return 0;
}
