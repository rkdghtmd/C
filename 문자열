======================================================
#1번 문제
======================================================
#include <stdio.h>

int main()
{
    char a;
    scanf("%c",&a);
    printf("%d",a);
    return 0;
}
======================================================
#3번 문제
======================================================
#include <stdio.h>

int main()
{
    char string[101];
    scanf("%s",string);
    for (int i = 'a'; i<='z'; i++)
    {
        for (int l = 0 ; string[l] != '\0'; l++)
        {
            if (string[l]==i)
            {
                printf("%d ",l);
                break;
            }
            if (string[l+1]=='\0')
            {
                printf("-1 ");
            }
        }
    }
}
======================================================
#4번 문제
======================================================
#include <stdio.h>

int main()
{
    int number;
    scanf("%d",&number);
    for (int i = 0 ; i < number ; i ++)
    {
        int num;
        char string[20];
        scanf("%d %s",&num,string);
        for (int i =0; string[i]!='\0';i++)
        {
            for (int l = 0 ; l < num ; l ++)
            {
                printf("%c",string[i]);
            }
        }
        printf("\n");
    }
}
======================================================
#5번 문제
======================================================
#include <stdio.h>

int main()
{

    char string[1000000];
    scanf("%s",string);
    for (int i = 0 ; string[i] != '\0'; i++)
    {
        if (string[i]>='a'&& string[i]<='z')
        {
            string[i]-=32;
        }
    }
    int num_max=0;
    int answer_case;
    for (int i = 'A' ;i<='Z'; i++)
    {
        int a=0;
        for (int l = 0 ; string[l]!= '\0'; l++)
        {
            if (string[l]==i)
            {
                a++;
            }
        }
        if (num_max<a)
        {
            num_max=a;
        }
    }
    int answer=0;
    for (int i = 'A' ;i<='Z'; i++)
    {
        int a=0;
        int l = 0;
        for (; string[l]!= '\0'; l++)
        {
            if (string[l]==i)
            {
                a++;
            }
        }
        if (num_max==a)
        {
            answer++;
            answer_case=i;
        }
    }
    if (answer==1)
    {
        printf("%c",answer_case);
    }
    else
    {
        printf("?");
    }

}
