#include <stdio.h>

int main()
{
    int number;
    scanf("%d",&number);
    for (int i =0; number>i;i++)
    {
        printf("%d\n",i+1);
    }
    return 0;
}
======================================================================================
#6번 문제
======================================================================================
#include <stdio.h>

int main()
{
    int number;
    scanf("%d",&number);
    for (int i =0; number>i;i++)
    {
        printf("%d\n",number-i);
    }
    return 0;
}
======================================================================================
#7번 
======================================================================================
#include <stdio.h>

int main()
{
    int i,number,a,b;
    scanf("%d",&number);
    for (i=1;i<number+1;i++)
    {
        scanf("%d %d",&a,&b);
        printf("Case #%d: %d\n",i,a+b);
    }
    return 0;
}
======================================================================================
#8번 문제
======================================================================================
#include <stdio.h>

int main()
{
    int i,number,a,b;
    scanf("%d",&number);
    for (i=1;i<number+1;i++)
    {
        scanf("%d %d",&a,&b);
        printf("Case #%d: %d + %d = %d\n",i,a,b,a+b);
    }
    return 0;
}
======================================================================================
#9번 문제
======================================================================================
#include <stdio.h>

int main()
{
    int number;
    scanf("%d",&number);
    for (int i =1; i<=number;i++)
    {
        for (int l =0; l<i;l++)
        {
        printf("*");

        }
    printf("\n");
    }
    
}
======================================================================================
#10번 문제
======================================================================================
#include <stdio.h>

int main()
{
    int number;
    scanf("%d",&number);
    for (int i =1 ; i<number+1; i++)
    {
        for (int m= number-i; m!=0 ; m--)
        {
            printf(" ");
        }
        for (int l=0; l<i;l++)
        {

            printf("*");
        }
        printf("\n");
    }
    return 0;
}
======================================================================================
#11번 문제
======================================================================================
#include <stdio.h>


int main()
{
    int a,b,c;
    scanf("%d %d",&a,&b);
    for (int i = 0 ; i<a; i++)
    {
        scanf("%d",&c);
        if (c<b)
        {
            printf("%d ",c);
        }
    printf("");
    }
    return 0;
}
