==================================================================================
#1번 문제
==================================================================================

#include <stdio.h>

int main ()
{
    int a,b;
    scanf("%d %d",&a,&b);
    if (a>b)
    {   
        printf(">");
    }
    if (a==b)
    {   
        printf("==");
    }    
    if (a<b)
    {   
        printf("<");
    }
}
==================================================================================
#2번 문제
==================================================================================
#include <stdio.h>

int main()
{
    int a;
    scanf("%d",&a);
    if (90<=a&&a<=100)
    {
        printf("A");
    }
    if (80<=a&&a<90)
    {
        printf("B");
    }
    if (70<=a&&a<80)
    {
        printf("C");
    }
    if (60<=a&&a<70)
    {
        printf("D");
    }
    if (a<60)
    {
        printf("F");
    }
    return 0;
}
